{
  "name": "ember-cli-ifa",
  "version": "0.10.0",
  "description": "Inject fingerprinted assetMap.json file into your app and provide initializer, service, and helper to dynamically reference fingerprinted assets.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:js": "eslint ./*.js app config tests",
    "start": "ember server",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test-no-fastboot": "FASTBOOT_DISABLED=true ember test",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/RuslanZavacky/ember-cli-ifa.git"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "author": {
    "name": "Ruslan Zavacky",
    "email": "ruslan.zavacky@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "^3.26.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-fastboot-testing": "^0.4.0",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-htmlbars-inline-precompile": "^3.0.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-qunit": "^4.3.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^8.0.2",
    "ember-source": "^3.27.2",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.5.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^8.13.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "qunit-dom": "^1.6.0"
  },
  "keywords": [
    "ember-addon",
    "fingerprinting",
    "fingerprint",
    "assetMap",
    "asset-map"
  ],
  "dependencies": {
    "ember-cli-babel": "^7.26.6"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
